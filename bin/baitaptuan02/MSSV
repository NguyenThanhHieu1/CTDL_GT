import java.util.Scanner;
public class ktraMaSoSV {
    public static void main(String[] args) {
        String MSSV;
        System.out.print("Nhập vào MSSV:");
        Scanner sc = new Scanner(System.in);
  
        MSSV = sc.nextLine(); // Nhận vào 1 chuỗi từ bàn phím
  
        if (MSSV.matches("2008110130")) //Kiểm tra bằng biểu thức chính quy
           System.out.println("Phù hợp");
        else
           System.out.println("Không phù hợp");
    }
    
}
